{
  "name": "ns-jasmine-core",
  "homepage": "https://jasmine.github.io",
  "authors": [
    "slackersoft <gregg@slackersoft.net>"
  ],
  "description": "Unofficial packaging of Jasmine's core files for use in NetSuite",
  "keywords": [
    "test",
    "jasmine",
    "tdd",
    "bdd",
    "netsuite"
  ],
  "license": "MIT",
  "moduleType": "globals",
  "main": "lib/jasmine-core/jasmine.js",
  "ignore": [
    "**/.*",
    "dist",
    "grunt",
    "node_modules",
    "pkg",
    "release_notes",
    "Gemfile",
    "Gemfile.lock",
    "Rakefile",
    "jasmine-core.gemspec",
    "*.sh",
    "*.py",
    "Gruntfile.js",
    "lib/jasmine-core.rb",
    "lib/jasmine-core/boot/",
    "lib/jasmine-core/spec",
    "lib/jasmine-core/version.rb",
    "lib/jasmine-core/*.py",
    "sauce_connect.log"
  ]
}
